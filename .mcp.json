{
  "mcpServers": {
    "Context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp@1.0.0"]
    },
    "mcp-feedback-enhanced": {
      "command": "uvx",
      "args": ["mcp-feedback-enhanced@latest"],
      "timeout": 600,
      "env": {
        "MCP_DESKTOP_MODE": "true",
        "MCP_WEB_HOST": "127.0.0.1",
        "MCP_WEB_PORT": "8765",
        "MCP_DEBUG": "false"
      },
      "autoApprove": ["interactive_feedback"]
    },
    "Sequential Thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking@1.0.0"]
    },
    "Shrimp Task Manager": {
      "command": "npx",
      "args": ["-y", "mcp-shrimp-task-manager@1.0.0"]
    },
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem@2.0.0",
        "${MCP_FS_PATH_1:-.}",
        "${MCP_FS_PATH_2:-$HOME/Desktop}",
        "${MCP_FS_PATH_3:-$HOME/Documents}"
      ]
    },
    "magic": {
      "command": "npx",
      "args": ["-y", "@21st-dev/magic@1.0.0"],
      "env": {
        "API_KEY": "${MAGIC_API_KEY}"
      }
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@playwright/mcp@0.0.20"]
    },
    "docker": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-server-docker@1.0.0"]
    },
    "mysql": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@benborla29/mcp-server-mysql@1.0.0"],
      "env": {
        "MYSQL_HOST": "${MYSQL_HOST:-localhost}",
        "MYSQL_PORT": "${MYSQL_PORT:-3306}",
        "MYSQL_USER": "${MYSQL_USER:-root}",
        "MYSQL_PASS": "${MYSQL_PASS}",
        "MYSQL_DB": "${MYSQL_DB:-test}"
      }
    },
    "magic-ui": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "magicui-mcp@1.0.0"]
    },
    "time-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "time-mcp@1.0.0"]
    },
    "redis-mcp": {
      "command": "npx",
      "args": [
        "-y",
        "@pickstar-2002/redis-mcp@1.0.0",
        "--host", "${REDIS_HOST:-localhost}",
        "--port", "${REDIS_PORT:-6379}",
        "--password", "${REDIS_PASS}",
        "--db", "${REDIS_DB:-0}"
      ]
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "chrome-devtools-mcp@1.0.0"]
    }
  }
}
